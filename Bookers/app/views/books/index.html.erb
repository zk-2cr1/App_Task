<h1>Books</h1>

<%@books.each do |book| %>

  <p>Title</p>
   <span><%= book.title %></span>


  <p>Body</p>
   <span><%= book.body %></span>

  <span>
    <%= link_to "Show", "book_path(book.id) " %>
    <%= link_to "Edit", "edit_book_path(book.id)" %>
    <%= link_to "Destory", "book_path(book.id) " %>

  </span>



<% end %>